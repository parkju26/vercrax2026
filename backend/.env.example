PORT=3000
NODE_ENV=development

# Providers
OPENAI_API_KEY=sk-...
ANTHROPIC_API_KEY=sk-ant-...

# Optional: force a default provider for all roles (openai|anthropic)
DEFAULT_PROVIDER=openai

# Supabase (server-only)
SUPABASE_URL=https://xxxxxxxxxxxxx.supabase.co
SUPABASE_SERVICE_ROLE_KEY=eyJ...

# Behavior
# When keys are missing, the system will fall back to a mock engine so you can test UI end-to-end.
ALLOW_MOCK_WHEN_NO_KEYS=true
